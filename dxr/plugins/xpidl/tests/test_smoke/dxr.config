[DXR]
enabled_plugins     = pygmentize xpidl
disabled_plugins    = clang
es_index            = dxr_test_{config_path_hash}_{format}_{tree}_{unique}
es_alias            = dxr_test_{config_path_hash}_{format}_{tree}
es_catalog_index    = dxr_test_{config_path_hash}_catalog
workers = 4

[code]
source_folder=code
object_folder=code/obj
build_command=cd $source_folder && make
clean_command=cd .. && make clean

  [[xpidl]]
  header_path=code/obj
  include_folders=code/base
